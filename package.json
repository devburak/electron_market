{
  "name": "elektron-datca",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "dist": "build",
    "dev": "nodemon --watch . --exec \"electron . --debug\""
  },
  "build": {
    "appId": "com.datca.app",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "deleteAppDataOnUninstall": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Elektron-Datca",
      "artifactName": "Elektron-Datca.Setup.${version}.${ext}"
    }
  },
  "devDependencies": {
    "electron": "^3.0.4",
    "electron-builder": "^20.39.0"
  },
  "dependencies": {
    "chart.js": "^2.8.0",
    "electron-store": "^2.0.0",
    "electron-unhandled": "^3.0.2",
    "flatpickr": "^4.5.7",
    "lowdb": "^1.0.0",
    "numeral": "^2.0.6",
    "print-js": "^1.0.63",
    "spectre.css": "^0.5.8"
  },
  "author": "Burak Ä°MREK",
  "license": "ISC"
}
